class produce {
  int id;
  String name;
  double price;
  produce(this.id, this.name, this.price);
  void display() {
    print('ID:$id,Name:$name,Price:$price');
  }
}

void main() {
  List<produce> products = [
    produce(1, 'tuyen', 12000),
    produce(2, 'tung', 10001),
    produce(3, 'trang', 000505)
  ];
  for (var product in products) {
    product.display();
  }
 }